    function addLiquidity(
        address tokenA,
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin,
        uint256 feeBipsA,
        uint256 feeBipsB,
        address to,
        uint256 deadline
    ) external returns (uint256 amountA, uint256 amountB, uint256 liquidity);

    function addLiquidityNAT(
        address token,
        uint256 amountTokenDesired,
        uint256 amountTokenMin,
        uint256 amountNATMin,
        uint256 feeBipsToken,
        address to,
        uint256 deadline
    ) external payable returns (uint256 amountToken, uint256 amountNAT, uint256 liquidity);

    function removeLiquidity(
        address tokenA,
        address tokenB,
        uint256 liquidity,
        uint256 amountAMin,
        uint256 amountBMin,
        address to,
        uint256 deadline
    ) external returns (uint256 amountA, uint256 amountB);

    function removeLiquidityNAT(
        address token,
        uint256 liquidity,
        uint256 amountTokenMin,
        uint256 amountNATMin,
        address to,
        uint256 deadline
    ) external returns (uint256 amountToken, uint256 amountNAT);

    function removeLiquidityWithPermit(
        address tokenA,
        address tokenB,
        uint256 liquidity,
        uint256 amountAMin,
        uint256 amountBMin,
        address to,
        uint256 deadline,
        bool approveMax,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external returns (uint256 amountA, uint256 amountB);

    function removeLiquidityNATWithPermit(
        address token,
        uint256 liquidity,
        uint256 amountTokenMin,
        uint256 amountNATMin,
        address to,
        uint256 deadline,
        bool approveMax,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external returns (uint256 amountToken, uint256 amountNAT);

    // Note:
    // Swap functions with exact input can be called with paths including fee-on-transfer tokens,
    // and the `amountOutMin` will be checked against what's actually been received by the `to` address.
    // Swap functions with exact output cannot be called with paths including fee-on-transfer tokes.
    function swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external returns (uint256[] memory amountsSent, uint256[] memory amountsRecv);

    function swapTokensForExactTokens(
        uint256 amountOut,
        uint256 amountInMax,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external returns (uint256[] memory amounts);

    function swapExactNATForTokens(
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external payable returns (uint256[] memory amountsSent, uint256[] memory amountsRecv);

    function swapTokensForExactNAT(
        uint256 amountOut,
        uint256 amountInMax,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external returns (uint256[] memory amounts);

    function swapExactTokensForNAT(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external returns (uint256[] memory amountsSent, uint256[] memory amountsRecv);

    function swapNATForExactTokens(
        uint256 amountOut,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external payable returns (uint256[] memory amounts);

    function quote(uint256 amountA, uint256 reserveA, uint256 reserveB) external pure returns (uint256 amountB);

    function getAmountOut(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) external pure returns (uint256 amountOut);

    function getAmountIn(
        uint256 amountOut,
        uint256 reserveIn,
        uint256 reserveOut
    ) external pure returns (uint256 amountIn);

    function getAmountsOut(uint256 amountIn, address[] calldata path) external view returns (uint256[] memory amounts);

    function getAmountsIn(uint256 amountOut, address[] calldata path) external view returns (uint256[] memory amounts);

    function pairFor(address tokenA, address tokenB) external view returns (address);

    function getReserves(address tokenA, address tokenB) external view returns (uint256, uint256);

    function selfPermit(address token, uint256 value, uint256 deadline, uint8 v, bytes32 r, bytes32 s) external;
}




// Contract definition inheriting ERC20, ERC20Burnable, Pausable, Ownable, ReentrancyGuard
contract FlareBear is ERC20, ERC20Burnable, Pausable, Ownable, ReentrancyGuard {
    uint256 public burnRate; // Percentage of tokens burned on each transfer
    uint256 public taxRate; // Percentage of tokens taxed on each transfer
    uint256 public lpRate; // Percentage of tokens allocated to LP rewards on each transfer
    address public lpWallet; // Address where LP rewards are transferred
    uint256 public totalRewardsDistributed; // Total rewards distributed to LP
    IBlazeSwapRouter public blazeSwapRouter; // BlazeSwap router contract instance
    address public wflrAddress; // WFLR token address for liquidity pairing

    // Track rewards and exclusion status for each address
    mapping(address => uint256) private lastTotalRewardsDistributed;
    mapping(address => uint256) private unclaimedRewards;
    mapping(address => bool) private isExcludedFromRewards;

    // Define events for transparency and external monitoring
    event RatesUpdated(uint256 newBurnRate, uint256 newTaxRate, uint256 newLpRate);
    event LpWalletUpdated(address newLpWallet);
    event RewardsClaimed(address claimant, uint256 amount);
    event RewardsExclusionUpdated(address account, bool isExcluded);
    event TokensWithdrawn(address to, uint256 amount);
    event Burn(address indexed from, uint256 amount);
    event LpTransfer(address indexed from, address indexed to, uint256 amount);
    event TaxCollected(address indexed from, uint256 amount);


    //blazeswapRouterAddress=0xe3A1b355ca63abCBC9589334B5e609583C7BAa06
    // wflrAddress=0x1D80c49BbBCd1C0911346656B529DF9E5c2F783d
    // Constructor to initialize contract with initial supply and rates
    constructor(address _blazeSwapRouter, address _wflrAddress) ERC20("FlareBear", "BEAR") {
        _mint(address(this), 77 * 10**27); // Minting initial supply

        // Set default rates (in basis points)
        burnRate = 300; // 3%
        taxRate = 200; // 2%
        lpRate = 500; // 5%

        // Set LP wallet to contract creator
        lpWallet = msg.sender;

        // Initialize BlazeSwap router contract instance
        blazeSwapRouter = IBlazeSwapRouter(_blazeSwapRouter);
        wflrAddress = _wflrAddress;

    }

    
    // Function to set LP wallet address, only callable by owner
    function setLpWallet(address newLpWallet) public onlyOwner {
        require(newLpWallet != address(0), "LP Wallet address cannot be the zero address");
        lpWallet = newLpWallet;
        emit LpWalletUpdated(newLpWallet); // Emit event here
    }

    // Function to set new burn, tax, and LP rates, only callable by owner
    function setRates(uint256 newBurnRate, uint256 newTaxRate, uint256 newLpRate) public onlyOwner {
        require(newBurnRate + newTaxRate + newLpRate <= 10000, "Combined rate must be <= 100%");
        burnRate = newBurnRate;
        taxRate = newTaxRate;
        lpRate = newLpRate;
        emit RatesUpdated(newBurnRate, newTaxRate, newLpRate); // Emit event here
    }

    function _transfer(address sender, address recipient, uint256 amount) internal override whenNotPaused {
    uint256 burnAmount = (amount * burnRate) / 10000;
    uint256 taxAmount = (amount * taxRate) / 10000;
    uint256 lpAmount = (amount * lpRate) / 10000;
    uint256 transferAmount = amount - burnAmount - taxAmount - lpAmount;

    // Burn tokens
    super._burn(sender, burnAmount);
    emit Burn(sender, burnAmount);

    // Transfer tax (update totalRewardsDistributed accordingly or as needed for your tax logic)
    totalRewardsDistributed += taxAmount;
    emit TaxCollected(sender, taxAmount);

    // Logic to handle liquidity directly with BlazeSwap
    if (lpAmount > 0 && address(blazeSwapRouter) != address(0)) {
        // Fetch current reserves for the token pair
        (uint256 reserveA, uint256 reserveB) = blazeSwapRouter.getReserves(address(this), wflrAddress);

        // Calculate the amount of wflr needed for the given lpAmount of this token
        uint256 amountWFLRNeeded = blazeSwapRouter.quote(lpAmount, reserveA, reserveB);

        // Check if the contract has enough WFLR tokens
        require(IERC20(wflrAddress).balanceOf(address(this)) >= amountWFLRNeeded, "Insufficient WFLR balance");

        // Approve the router to spend this contract's token and WFLR
        _approve(address(this), address(blazeSwapRouter), lpAmount);
        IERC20(wflrAddress).approve(address(blazeSwapRouter), amountWFLRNeeded);

        // Add liquidity to BlazeSwap
blazeSwapRouter.addLiquidity(
    address(this),          // tokenA
    wflrAddress,            // tokenB
    lpAmount,               // amountADesired for tokenA
    amountWFLRNeeded,       // amountBDesired for tokenB
    0,                      // amountAMin: Minimum tokenA amount for slippage tolerance
    0,                      // amountBMin: Minimum tokenB amount for slippage tolerance
    0,                      // feeBipsA: Fee in basis points for tokenA
    0,                      // feeBipsB: Fee in basis points for tokenB
    address(this),          // to: Receiver of the liquidity tokens
    block.timestamp         // deadline: Transaction must occur before this time
);

    }

    // Transfer the net amount to the recipient
    super._transfer(sender, recipient, transferAmount);
}


    // Function to allow users to claim LP rewards
    function claimRewards() public nonReentrant {
        _updateRewards(msg.sender); // Update rewards for caller

        uint256 rewards = unclaimedRewards[msg.sender]; // Get unclaimed rewards
        require(rewards > 0, "No rewards to claim"); // Ensure there are rewards to claim

        // Reset unclaimed rewards and transfer them to caller
        unclaimedRewards[msg.sender] = 0;
        super._transfer(address(this), msg.sender, rewards); // Transfer rewards from the contract to the msg.sender
        emit RewardsClaimed(msg.sender, rewards); // Emit event after rewards are claimed
    }

    // Internal function to update rewards for an address
    function _updateRewards(address account) private {
        if (isExcludedFromRewards[account]) {
            // Skip updating rewards for excluded addresses
            return;
        }

        uint256 totalDistributed = totalRewardsDistributed - lastTotalRewardsDistributed[account];
        if (totalDistributed > 0) {
            // Calculate and update unclaimed rewards for the account
            unclaimedRewards[account] += (balanceOf(account) * totalDistributed) / totalSupply();
            lastTotalRewardsDistributed[account] = totalRewardsDistributed;
        }
    }

    // Function to get current rates
    function getRates() public view returns (uint256 currentBurnRate, uint256 currentTaxRate, uint256 currentLpRate) {
        return (burnRate, taxRate, lpRate);
    }

    // Function to get last total rewards distributed for an address
    function getLastTotalRewardsDistributed(address account) public view returns (uint256) {
        return lastTotalRewardsDistributed[account];
    }

    // Function to get unclaimed rewards for an address
    function getUnclaimedRewards(address account) public view returns (uint256) {
        return unclaimedRewards[account];
    }

    // Function to get total supply details
    function totalSupplyDetails() public view returns (uint256 currentTotalSupply, uint256 tokensBurned, uint256 tokensForRewards) {
        uint256 burnedSupply = totalSupply() - balanceOf(address(this)) - balanceOf(lpWallet);
        return (totalSupply(), burnedSupply, balanceOf(address(this)));
    }

    // Function to get LP wallet address
    function getLpWallet() public view returns (address) {
        return lpWallet;
    }

    // Function to get total rewards distributed
    function getTotalRewardsDistributed() public view returns (uint256) {
        return totalRewardsDistributed;
    }

    // Function to exclude or include an address from LP rewards, only callable by owner
    function excludeFromRewards(address account, bool excluded) public onlyOwner {
        isExcludedFromRewards[account] = excluded;
        emit RewardsExclusionUpdated(account, excluded); // Emit event here
    }

    // Function to check if an address is excluded from LP rewards
    function isExcludedFromReward(address account) public view returns (bool) {
        return isExcludedFromRewards[account];
    }

    // Function to calculate total rewards (claimed + pending) for an address
    function rewardsOf(address account) public view returns (uint256) {
        uint256 totalDistributed = totalRewardsDistributed - lastTotalRewardsDistributed[account];
        uint256 pendingRewards = (balanceOf(account) * totalDistributed) / totalSupply();
        return unclaimedRewards[account] + pendingRewards;
    }

    // Function to withdraw tokens from the contract, only callable by owner
    function withdrawTokens(address to, uint256 amount) public onlyOwner {
        require(amount <= balanceOf(address(this)), "Insufficient balance in contract");
        _transfer(address(this), to, amount);
        emit TokensWithdrawn(to, amount); // Emit event here
    }

    // Function to pause token transfers, only callable by owner
    function pause() public onlyOwner {
        _pause();
    }

    // Function to unpause token transfers, only callable by owner
    function unpause() public onlyOwner {
        _unpause();
    }
}